{{#power-select
  options=options
  selected=selected
  searchField="name"
  placeholder=placeholder
  onchange=onchange
  matcher=matcher
  oninput=(action 'handleSearch')
  as |district|
}}
  <strong class="district">{{district.boro}} {{district.cd}}</strong> <small class="neighborhoods">{{district.neighborhoods}}</small>
{{/power-select}}
