{{#mapbox-gl id='lu-map' initOptions=(hash style='mapbox://styles/mapbox/light-v9' zoom=zoom center=(array lng lat)) as |map|}}
  {{#map.source sourceId='pluto' source=rasterSourceOptions as |source|}}
    {{source.layer layer=rasterLayerOptions}}
  {{/map.source}}
  {{map.call 'fitBounds' mapState.bounds}}
  {{#if mapState.currentlySelected}}
    {{#map.source sourceId='currentlySelected' source=cdSelectedSource as |source|}}
      {{source.layer layer=cdSelectedLayer}}
    {{/map.source}}
  {{/if}}
{{/mapbox-gl}}
{{yield}}
